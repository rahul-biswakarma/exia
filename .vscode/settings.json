{
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.formatOnPaste": true
  },
  "rust-analyzer.rustfmt.enable": true,
  "rust-analyzer.rustfmt.overrideCommand": ["rustfmt", "--edition", "2021"],
  "cSpell.words": [
    "datalink",
    "dioxus",
    "hotspots",
    "iface",
    "ollama",
    "onclick",
    "oninput",
    "onsubmit",
    "qdrant"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ]
}
